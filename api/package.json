{
	"name": "api",
	"version": "1.0.0",
	"main": "src/index.ts",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"lint": "eslint --ext .ts --ext .tsx .",
		"build": "tsc  --project . --outDir ./dist",
		"start": "node --es-module-specifier-resolution=node dist/index.js",
		"dev": "tsc-watch --project . --outDir ./dist --onSuccess \"nodemon\"",
		"docker:run": "(docker-compose down; docker-compose build; docker-compose up)",
		"docker:run:windows": "(docker compose down; docker compose build; docker compose up)",
		"docker:image": "docker build -t node-api:v1 .",
		"docker:network": "docker network create node-api-network"
	},
	"dependencies": {
		"express": "^4.17.2",
		"mysql2": "^2.3.3"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/jest": "^27.4.0",
		"@types/node": "^16.11.7",
		"@types/prettier": "^2.4.1",
		"@typescript-eslint/eslint-plugin": "^5.3.1",
		"@typescript-eslint/parser": "^5.3.1",
		"eslint": "^8.2.0",
		"eslint-config-airbnb-typescript": "^15.0.0",
		"eslint-config-airbnb-typescript-prettier": "^4.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^5.1.1",
		"jest": "^27.4.7",
		"nodemon": "^2.0.14",
		"prettier": "^2.4.1",
		"ts-jest": "^27.1.2",
		"ts-node": "^10.4.0",
		"tsc-watch": "^4.6.0",
		"tsconfig-paths": "^3.11.0",
		"typescript": "^4.4.4"
	}
}
